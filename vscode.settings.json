{
    "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.lineNumbers": "relative",
    "editor.renderWhitespace": "boundary",
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/CVS": true,
        "**/dist": true,
        "**/node_modules": true,
        "**/tmp": true
    },
    "vim.easymotion": true,
    "vim.enableNeovim": true,
    "vim.leader": ",",
    "vim.neovimPath": "/usr/local/bin/nvim",
    /*
    experimental https://github.com/VSCodeVim/Vim/blob/master/README.md#vimstatusbarcolorcontrol
    */
    "vim.statusBarColorControl": false,
    "vim.statusBarColors": {
        "insert": "#5f0000",
        "normal": "#005f5f",
        "replace": "#000000",
        "visual": "#5f00af",
        "visualblock": "#86592d",
        "visualline": "#005f87"
    },
    "vsicons.presets.jsOfficial": true,
    "vsicons.presets.jsonOfficial": true,
    "vsicons.presets.tsOfficial": true,
    "workbench.colorTheme": "Cobalt2",
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorCustomizations": {
        "editorIndentGuide.activeBackground": "#ffc600"
    },
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.useSystemClipboard": true,
    "window.zoomLevel": 0,
    "vsicons.dontShowConfigManuallyChangedMessage": true,
    "terminal.external.osxExec": "iTerm.app",
    "eslint.autoFixOnSave": true,
    "html.format.indentHandlebars": true,
    "html.suggest.angular1": false,
    "html.suggest.ionic": false,
    "search.useIgnoreFiles": true,
    "tslint.autoFixOnSave": true,
    "workbench.editor.labelFormat": "medium",
    "alex.enable": true,
    "gitlens.keymap": "alternate",
    "vsicons.dontShowNewVersionMessage": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "telemetry.enableTelemetry": false,
    "gitlens.historyExplorer.enabled": true,
    "sync.gist": "31fd799d5fba67fb544c31683334695c",
    "sync.host": "",
    "sync.pathPrefix": "",
    "sync.quietSync": false,
    "sync.askGistName": false,
    "sync.removeExtensions": true,
    "sync.syncExtensions": true,
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.lastUpload": "2018-08-14T09:15:48.819Z",
    "sync.lastDownload": "2018-06-21T23:00:57.475Z",
    "sync.forceDownload": false,
    "workbench.sideBar.location": "right",
    "tslint.run": "onSave",
    "terminal.integrated.rendererType": "dom",
    "editor.formatOnSave": true,
    "prettier.requireConfig": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "multiCommand.commands": [
        {
            "command": "multiCommand.makeRoom",
            "sequence": [
                "workbench.action.toggleSidebarVisibility",
                "workbench.action.toggleActivityBarVisibility"
            ]
        }
    ],
    "workbench.activityBar.visible": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "editor.tabSize": 2,
}