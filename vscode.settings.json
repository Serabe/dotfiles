{
    "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.lineNumbers": "relative",
    "editor.renderWhitespace": "boundary",
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/CVS": true,
        "**/dist": true,
        "**/node_modules": true,
        "**/tmp": true
    },
    "vim.easymotion": true,
    "vim.enableNeovim": false,
    "vim.leader": ",",
    "vim.neovimPath": "/usr/local/bin/nvim",
    /*
    experimental https://github.com/VSCodeVim/Vim/blob/master/README.md#vimstatusbarcolorcontrol
    */
    "vim.statusBarColorControl": false,
    "vim.statusBarColors": {
        "insert": "#5f0000",
        "normal": "#005f5f",
        "replace": "#000000",
        "visual": "#5f00af",
        "visualblock": "#86592d",
        "visualline": "#005f87"
    },
    "vsicons.presets.jsOfficial": true,
    "vsicons.presets.jsonOfficial": true,
    "vsicons.presets.tsOfficial": true,
    "workbench.colorTheme": "Eagle Oceanic Next",
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.useSystemClipboard": true,
    "window.zoomLevel": 0,
    "vsicons.dontShowConfigManuallyChangedMessage": true,
    "terminal.external.osxExec": "iTerm.app",
    "eslint.autoFixOnSave": true,
    "html.format.indentHandlebars": true,
    "html.suggest.angular1": false,
    "html.suggest.ionic": false,
    "search.useIgnoreFiles": true,
    "tslint.autoFixOnSave": true,
    "workbench.editor.labelFormat": "medium",
    "alex.enable": true,
    "gitlens.keymap": "alternate",
    "vsicons.dontShowNewVersionMessage": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "telemetry.enableTelemetry": false
}
